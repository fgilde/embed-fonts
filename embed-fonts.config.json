{
  "skipCertificateCheck": false,
  "placeholders": {
    "localDir": "./test-styles/external",
    "files": [
      "./node_modules/@sw/angular-controls/assets/styles/themes/default-theme.css",
      "https://unpkg.com/material-components-web@4.0.0/dist/material-components-web.min.css",
      "https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css",
      "https://fonts.googleapis.com/icon?family=Material+Icons",
      "https://fonts.googleapis.com/css?family=Open+Sans:300,400,700"
    ]
  },
  "bundledOutputFile" : "{localDir}/_all_bundle.css",
  "mappings": [
    {
      "minify": true,
      "from": "{files}",
      "to": "{localDir}"
    }
  ]
}